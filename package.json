{
  "name": "react-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --names \"app,codegen\" \"npm run gen-type\" \"npm run dev:app\"",
    "dev:app": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:analyze": "vite build --mode analyze",
    "gen-type": "dotenv -- supabase gen types typescript --project-id=\"$SUPABASE_PROJECT_ID\" > src/types/supabase.ts"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@supabase/supabase-js": "^2.49.4",
    "antd": "^5.24.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.23.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "supabase": "^2.20.12",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite": "6.2.6",
    "concurrently": "^9.1.0"
  }
}
